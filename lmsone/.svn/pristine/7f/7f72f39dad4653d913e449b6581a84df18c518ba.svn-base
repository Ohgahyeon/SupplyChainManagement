package kr.happyjob.study.mng.model;

import java.util.Date;

public class QnaReplyDto {
	
	private int reply_No;
	private int board_No;
	private String loginID;
	private String reply_Content;
	private Date reply_Date;
	private String name;
	private String user_Type;
	
	public QnaReplyDto() {
		super();
		// TODO Auto-generated constructor stub
	}

	public QnaReplyDto(int reply_No, int board_No, String loginID, String reply_Content, Date reply_Date, String name,
			String user_Type) {
		super();
		this.reply_No = reply_No;
		this.board_No = board_No;
		this.loginID = loginID;
		this.reply_Content = reply_Content;
		this.reply_Date = reply_Date;
		this.name = name;
		this.user_Type = user_Type;
	}

	public int getReply_No() {
		return reply_No;
	}

	public void setReply_No(int reply_No) {
		this.reply_No = reply_No;
	}

	public int getBoard_No() {
		return board_No;
	}

	public void setBoard_No(int board_No) {
		this.board_No = board_No;
	}

	public String getLoginID() {
		return loginID;
	}

	public void setLoginID(String loginID) {
		this.loginID = loginID;
	}

	public String getReply_Content() {
		return reply_Content;
	}

	public void setReply_Content(String reply_Content) {
		this.reply_Content = reply_Content;
	}

	public Date getReply_Date() {
		return reply_Date;
	}

	public void setReply_Date(Date reply_Date) {
		this.reply_Date = reply_Date;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getUser_Type() {
		return user_Type;
	}

	public void setUser_Type(String user_Type) {
		this.user_Type = user_Type;
	}

	@Override
	public String toString() {
		return "QnaReplyDto [reply_No=" + reply_No + ", board_No=" + board_No + ", loginID=" + loginID
				+ ", reply_Content=" + reply_Content + ", reply_Date=" + reply_Date + ", name=" + name + ", user_Type="
				+ user_Type + "]";
	}
	
}
