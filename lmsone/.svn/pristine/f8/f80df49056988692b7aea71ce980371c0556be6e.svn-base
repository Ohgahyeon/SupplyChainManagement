package kr.happyjob.study.sup.model;

import java.util.Date;

public class CourseModel  {

  private int class_no;             // 강의번호
  private String loginID;           // loginID
  private String name;              // 이름
  private String class_nm;		    // 과정명
  private String class_plan;		// 강의계획
  private String class_startdate; 	// 강의시작일
  private String class_enddate;     // 강의종료일
  private Date recv_startdate;      // 모집시작일
  private Date recv_enddate;        // 모집종료일
  private int class_totalcnt;       // 정원
  private int user_cnt;             // 신청인원
  
	@Override
	public String toString() {
		return "CourseModel [class_no=" + class_no + ", loginID=" + loginID + ", name=" + name + ", class_nm=" + class_nm
				+ ", class_plan=" + class_plan + ", class_startdate=" + class_startdate + ", class_enddate=" + class_enddate
				+ ", recv_startdate=" + recv_startdate + ", recv_enddate=" + recv_enddate + ", class_totalcnt="
				+ class_totalcnt + ", user_cnt=" + user_cnt + "]";
	}
	
	public int getClass_no() {
		return class_no;
	}
	
	public void setClass_no(int class_no) {
		this.class_no = class_no;
	}
	
	public String getLoginID() {
		return loginID;
	}
	
	public void setLoginID(String loginID) {
		this.loginID = loginID;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getClass_nm() {
		return class_nm;
	}
	
	public void setClass_nm(String class_nm) {
		this.class_nm = class_nm;
	}
	
	public String getClass_plan() {
		return class_plan;
	}
	
	public void setClass_plan(String class_plan) {
		this.class_plan = class_plan;
	}
	
	public String getClass_startdate() {
		return class_startdate;
	}
	
	public void setClass_startdate(String class_startdate) {
		this.class_startdate = class_startdate;
	}
	
	public String getClass_enddate() {
		return class_enddate;
	}
	
	public void setClass_enddate(String class_enddate) {
		this.class_enddate = class_enddate;
	}
	
	public Date getRecv_startdate() {
		return recv_startdate;
	}
	
	public void setRecv_startdate(Date recv_startdate) {
		this.recv_startdate = recv_startdate;
	}
	
	public Date getRecv_enddate() {
		return recv_enddate;
	}
	
	public void setRecv_enddate(Date recv_enddate) {
		this.recv_enddate = recv_enddate;
	}
	
	public int getClass_totalcnt() {
		return class_totalcnt;
	}
	
	public void setClass_totalcnt(int class_totalcnt) {
		this.class_totalcnt = class_totalcnt;
	}
	
	public int getUser_cnt() {
		return user_cnt;
	}
	
	public void setUser_cnt(int user_cnt) {
		this.user_cnt = user_cnt;
	}

  

}
