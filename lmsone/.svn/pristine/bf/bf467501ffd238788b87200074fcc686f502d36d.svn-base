package kr.happyjob.study.cmg.model;

import java.util.Date;

public class ClassroomModel {

	private int roomNo;			// 강의실번호
	private String roomNm;		// 강의실명
	private String roomArea;	// 크기
	private String roomSize;	// 자리수
	private String roomEtc;		// 비고
	private int classnum;			// 강의수
	
	public int getRoomNo() {
		return roomNo;
	}
	public void setRoomNo(int roomNo) {
		this.roomNo = roomNo;
	}
	public String getRoomNm() {
		return roomNm;
	}
	public void setRoomNm(String roomNm) {
		this.roomNm = roomNm;
	}
	public String getRoomArea() {
		return roomArea;
	}
	public void setRoomArea(String roomArea) {
		this.roomArea = roomArea;
	}
	public String getRoomSize() {
		return roomSize;
	}
	public void setRoomSize(String roomSize) {
		this.roomSize = roomSize;
	}
	public String getRoomEtc() {
		return roomEtc;
	}
	public void setRoomEtc(String roomEtc) {
		this.roomEtc = roomEtc;
	}
	public int getClassnum() {
		return classnum;
	}
	public void setClassnum(int classnum) {
		this.classnum = classnum;
	}
	
}