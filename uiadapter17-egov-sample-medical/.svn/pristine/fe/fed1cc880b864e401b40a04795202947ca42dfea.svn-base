this.fn_checkId = function(sValue)
{
    var sRtn = "N";
    var sRegExp = "[a-z0-9]{4,15}";
    var objRegExp = new RegExp(sRegExp, "i"); 
    var objResult = objRegExp.exec(sValue);
	
    if (objResult == null)
    {
        sRtn = "N";
    }
    else
    {
        if ((objResult.index == 0) && (objResult == sValue))
            sRtn = "Y";
        else
            sRtn = "N";
    }

    return sRtn;


};



