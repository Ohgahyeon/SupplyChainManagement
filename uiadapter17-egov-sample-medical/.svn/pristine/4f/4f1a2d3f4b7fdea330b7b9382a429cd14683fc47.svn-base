 	this.gfnCommCdCombo('areaCD',this.cbo_workarea3);
	
	
	
	this.cbo_grade.set_displaynulltext("초급");
	this.cbo_career_year.set_displaynulltext("0");
	this.cbo_career_month.set_displaynulltext("0");
	this.cbo_tel1.set_displaynulltext("010");
	this.userinfo();
	
	
};

this.userinfo=function(){

	var strSvcId    = "userinfo";
	var strSvcUrl   = "userinfo.do";
	var inData      = "";
	var outData     = "ds_Resultlist=ds_Resultlist   ds_skill=ds_skill";
	var strArg      = "loginID="+userID + " userType="+userType;
	var callBackFnc = "fnCallback";
	this.ds_Resultlist.setColumn(0,"loginID", nexacro.getEnvironmentVariable("gvUserId"))//gvUserId
	this.ds_Resultlist.setColumn(1,"name",nexacro.getEnvironmentVariable("ds_Resultlist"))
	//생략가능
	var isAsync   = true;
	var nDataType = 0;	
	
	this.gfnTransaction( strSvcId , 	// transaction을 구분하기 위한 svc id값
						 strSvcUrl , 	// trabsaction을 요청할 주소
						 inData , 		// 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
						 outData , 		// 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
						 strArg, 			// 입력갑스로 보낼 arguments, strFormData="20120607"
						 callBackFnc, 	// transaction의 결과를 받을 Function 이름
						 nDataType); // 통신방법 정의 [생략가능]
					
						
};


/**************************************************************************************************
* CallBack Function (서버수신)
***************************************************************************************************/
