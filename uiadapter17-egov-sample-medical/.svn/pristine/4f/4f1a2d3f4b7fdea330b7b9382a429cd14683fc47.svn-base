this.btn_reg_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
    trace("LoginID="+nexacro.getEnvironmentVariable("gvUserId"));

	if (this.confirm("수정하시겠습니까?"))
	{
			
			
			
			var strSvcId    = "userregpopup";
			var strSvcUrl   = "userregpopup.do";
			var inData      = "dslist=ds_list  dsSkill=dsskill:u";
			var outData     = "";
			var strArg      = "LoginID="+nexacro.getEnvironmentVariable("gvUserId");
			var callBackFnc = "fnCallback";
			
			
			//생략가능
			var isAsync   = true;
			var nDataType = 0;	
			
			this.gfnTransaction( strSvcId , 	// transaction을 구분하기 위한 svc id값
										strSvcUrl , 	// trabsaction을 요청할 주소
										inData , 		// 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
										outData , 		// 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
										strArg, 			// 입력갑스로 보낼 arguments, strFormData="20120607"
										callBackFnc, 	// transaction의 결과를 받을 Function 이름
										nDataType); // 통신방법 정의 [생략가능]



		
	}
	
	
};


