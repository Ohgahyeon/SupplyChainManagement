	//var gvServerUrl = nexacro.getEnvironmentVariable("gvServerUrl");
		var strSvcId    = "ProjectDetail";
		var strSvcUrl   = "ProjectDetail.do";
		var inData      = "";
		var outData     = "ds_ProjcetInfo=ds_ProjcetInfo ds_project_skill=ds_project_skill";
		var strArg      = "projectId="+projectId;
		var callBackFnc = "fnCallback";
	 
		
		//alert(nexacro.getEnvironmentVariable("gvUserId"));	
		//trace(this.ds_project_skill.saveXML());
		//alert(this.ds_ResultGroupCD.saveXML());
		
		//생략가능
		var isAsync   = true;
		var nDataType = 0;	
		
		this.gfnTransaction( strSvcId , 	// transaction을 구분하기 위한 svc id값
